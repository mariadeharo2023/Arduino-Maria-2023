//*********************** VARIABLES *****************************
const byte xiulet = 6;
//************************* SETUP *******************************
void setup() {              // configura el final de la salida
 pinMode(xiulet, OUTPUT);
}
//************************* LOOP ********************************
void loop() {               // inicia el bucle del programa

 tone(xiulet, 5000, 1000);

 delay(1000);
 
 tone(xiulet, 4000, 1000);

 delay(1000);

 tone(xiulet, 3000, 1000);

 delay(1000);

 tone(xiulet, 2000, 1000);

 delay(2000);

}
