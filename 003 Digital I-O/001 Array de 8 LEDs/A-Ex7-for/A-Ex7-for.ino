const byte led =  7;
int e;
//************************* SETUP *******************************
void setup() { 
  pinMode(led, OUTPUT);
  for (e= 0; e<30; e++)
  {
    digitalWrite(led, HIGH);
    delay(100);
    digitalWrite(led, LOW);
    delay(100);
  }
}
